    "build": "tsc"
  },
  "keywords": [
    "reproduce",
    "nodejs"
  ],
  "author": "Matteo Gioioso",
  "license": "MIT",
  "dependencies": {
    "flatted": "^3.1.1",
    "shimmer": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=13.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.72",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.20",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.31",
    "@types/pg": "^7.14.10",
    "@types/shimmer": "^1.0.1",
    "axios": "^0.21.1",
    "got": "^11.8.2",
    "husky": "^4.0.0",
    "jest": "^26.6.3",
    "knex": "^0.95.5",
    "mongodb": "^3.6.6",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "prettier": "^2.2.1",
    "sequelize": "^6.6.2",
    "superagent": "^6.1.0",
    "ts-jest": "^26.5.2",
